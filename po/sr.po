#: ../src/calendar/calendar.vala:151
#, no-c-format
#| msgid "%a, %b %e, %Y"
msgid "%b %e, %Y"
msgstr "%e. %b. %Y."

#. A "pretty" date with no day of week or year according to locale preferences, i.e. "March 10"
